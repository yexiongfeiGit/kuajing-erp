{
  "manifest_version": 3,
  "name": "跨境ERP助手",
  "description": "跨境ERP助手，支持采集100+货源平台商品，批量发布至数十家跨境电商平台店铺，支持一键代发采购，自动关联并同步采购单至货代系统",
  "version": "5.0.6",
  "icons": {
    "16": "icon/16.png",
    "24": "icon/24.png",
    "48": "icon/48.png",
    "128": "icon/128.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "scripting",
    "cookies",
    "declarativeNetRequest",
    "webRequest",
    "management",
    "unlimitedStorage"
  ],
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "*://*.szchengji-inc.com/*",
      "*://*.chengji-inc.com/*",
      "*://*.kuajing-plw.happyporsche.com/*",
      "*://*.91miaoshou.com/*"
    ]
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "injected.js",
        "env.json",
        "content-scripts/content.css",
        "rules.json",
        "icon/*"
      ],
      "matches": [
        "<all_urls>"
      ]
    },
    {
      "resources": [
        "content-scripts/content.css"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  },
  "action": {
    "default_title": "跨境ERP助手",
    "default_icon": "icon/16.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true,
      "run_at": "document_end",
      "js": [
        "content-scripts/content.js"
      ]
    }
  ],
  "key": ""
}